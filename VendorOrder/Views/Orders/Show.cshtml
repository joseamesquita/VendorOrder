@{
  Layout = "_Layout";
}
<div class="text-center">
  <h1 class="text-center">Vendor Order Details:</h1>
  <h4>Vendor: @Model["vendor"].Name</h4>
  <h4>Description: @Model["vendor"].Description</h4>
  <h4>Contact: @Model["vendor"].Contact</h4>
  <h4>Title: @Model["order"].Title</h4>
  <h4>Description @Model["order"].OrderDescription</h4>
  <h4>Date: @Model["order"].Date</h4>
  <h4>Price: $@Model["order"].Price</h4>
</div>