@{
  Layout = "_Layout";
}
<div class="text-center">
  <h1 class="text-center">Vendor Order Details:</h1>
  <h4>Vendor Name: @Model["vendor"].Name</h4>
  <h4>Vendor Description: @Model["vendor"].Description</h4>
  <h4>Vendor Contact: @Model["vendor"].Contact</h4>
  <h4>Order Title: @Model["order"].Title</h4>
  <h4>Order Description @Model["order"].OrderDescription</h4>
  <h4>Order Date: @Model["order"].Date</h4>
  <h4>Order Price: $@Model["order"].Price</h4>
</div>