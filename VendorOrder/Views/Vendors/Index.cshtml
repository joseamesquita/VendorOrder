
@{
  Layout = "_Layout";
}
<div class="col-md-8 offset-md-2">

  <h1 class="text-center">Vendors</h1>
  
  @if (Model.Count == 0)
  {
    <h3>No vendors have been added yet!</h3>
  }
  <ul class="list-group">
    @foreach (var vendors in Model)
  { 
    <li class="list-group-item">Title: @vendors.Title</li>
    <a href="/vendors/@vendors.Id"><button class="btn btn-success btn-lg btn-block">Show</button></a>
  <br>
  }
  
  </ul>
</div>